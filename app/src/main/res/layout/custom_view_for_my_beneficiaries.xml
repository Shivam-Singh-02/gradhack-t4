<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/beneficiaryNameTextView"
        android:layout_width="409dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="302dp"
        app:layout_constraintBottom_toTopOf="@+id/beneficiaryAccountNumberTextView"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="1dp" />

    <TextView
        android:id="@+id/beneficiaryAccountNumberTextView"
        android:layout_width="409dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        app:layout_constraintBottom_toTopOf="@+id/beneficiaryBankNameTextView"
        app:layout_constraintTop_toBottomOf="@+id/beneficiaryNameTextView"
        tools:layout_editor_absoluteX="1dp" />

    <TextView
        android:id="@+id/beneficiaryBankNameTextView"
        android:layout_width="409dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="357dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/beneficiaryAccountNumberTextView"
        tools:layout_editor_absoluteX="1dp" />
</android.support.constraint.ConstraintLayout>