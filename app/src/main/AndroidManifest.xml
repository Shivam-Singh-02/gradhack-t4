<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="t18.gradhack.com.main">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="t18.gradhack.com.bankservices.FundsTransferActivity"></activity>
        <activity android:name="t18.gradhack.com.main.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="t18.gradhack.com.main.MenuActivity" />

        <service
            android:name="t18.gradhack.com.main.TextToSpeechService"
            android:enabled="true"
            android:exported="true" />
        <service
            android:name="t18.gradhack.com.main.FingerprintAuthService"
            android:enabled="true"
            android:exported="true" />
    </application>

    <uses-feature
        android:name="android.hardware.fingerprint"
        android:required="false" />

    <uses-permission android:name="android.permission.USE_FINGERPRINT" />

</manifest>